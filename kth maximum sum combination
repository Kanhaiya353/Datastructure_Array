#include<iostream>
#include<stack>
using namespace std;
int main()
{
	int a[]={20,-5,-1};
	int n=sizeof(a)/sizeof(a[0]);
	int sum,t=0;

	for(int i=0;i<n;i++)
	{
		sum=0;
		for(int j=i;j<n;j++)//20,15,14,-6,-1;
		{
			
		sum=sum+a[j];
//		cout<<sum<<"  ";
		
		t++;
		if(t==3)
		{
			cout<<sum;
		}
		
		
		}
		
	}

	
	
	
	
	
}
