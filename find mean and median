#include<bits/stdc++.h>
using namespace std;
int main()
{
	int a[]={1,3,4,2,6,5,8,7};
	int n=sizeof(a)/sizeof(a[0]),sum=0;
	for(int i=0;i<n;i++)
	{
		sum+=a[i];
	}
	float mean=sum/(float)n;
	cout<<mean<<" ";
	
	sort(a,a+n);
	//1,2,3,4,5,6,7,8
	
		if(n%2!=0)
		{
			cout<<(double)(a[n/2]);
				}
			
		else{
			 
			 cout<<(double)(a[(n-1)/2]+a[n/2])/2.0;
			}
}
