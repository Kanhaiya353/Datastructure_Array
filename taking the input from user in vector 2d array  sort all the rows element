#include<iostream>
#include<vector>
#include<algorithm>
using namespace std;
int main()
{
	
//	vector< vector<int> >vect{{10,20,30,40}, 
//	                          {15,25,35,45},
//							  {25,29,37,48},
//							  {32,33,39,50}};
vector<vector<int>>vect;
int k,temp;
cout<<"enter the N mnuber of rows and columns";
cin>>k;
cout<<"prefenyuyuce";
for(int i=0;i<k;i++)
{
	vector<int>row;
	for(int j=0;j<k;j++)
	{
		cin>>temp;
//		row.push_back(temp);
  row.push_back(temp);
	}
	vect.push_back(row);
}

	
	int m=vect.size();//sizeof row;
	int n=vect[0].size();//size of column;
	for(int i=0;i<m;i++)
	{
		for(int j=0;j<n;j++)
		{
			cout<<vect[i][j]<<"  ";
		}
		cout<<endl;
	}
	for(int i=0;i<m;i++)
	{
	
//	sort(vect[i].begin(),vect[i].end());
  }
  sort(vect.begin(),vect.end());
	for(int i=0;i<m;i++)
	{
		for(int j=0;j<n;j++)
		{
			cout<<vect[i][j]<<"  ";
		}
		cout<<endl;
	}
	
}
