#include<iostream>
using namespace std;
void swap(int *p,int *q)
{int temp;
temp=*p;

*p=*q;
*q=temp;
	
}
int main()
{
	int a[]={9,4,7,3};
	int n=sizeof(a)/sizeof(a[0]);
	for(int j=0;j<n;j++)
	{
	
	for(int i=0;i<n-1;i++)
	{
		if(a[i]>a[i+1])
		{
		
		swap(&a[i],&a[i+1]);
	   }
	
	}}
	for(int i=0;i<n;i++)
	{
		cout<<a[i];
	}
	
}
