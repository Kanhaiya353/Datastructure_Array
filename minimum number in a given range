//{7,2,3,0,5,10,3,12,18}
//0,4  4,7  7,8
#include<iostream>
using namespace std;
int  mini(int a[],int b,int d)
{
	int min;
	for(int i=b;i<d;i++)
	{
		min=a[b];
		if(min>a[i])
		{ 
		   min=a[i];	
		}
	}
cout<<min<<" ";
	
}
int main()
{
	int a[]={7,2,3,0,5,10,3,12,18};
	int n=sizeof(a)/sizeof(a[0]);
	mini(a,0,4);
	mini(a,4,7);
	mini(a,7,8);
 } 
