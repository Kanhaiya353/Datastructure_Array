#include<iostream>
#include<vector>
#include<algorithm>
using namespace std;
int main()
{
	vector< vector<int> >vect{{9,8,7}, 
	                          {5,4,3},
							  {3,2,1}};
	
	int m=vect.size();//sizeof row;
	int n=vect[0].size();//size of column;
	for(int i=0;i<m;i++)
	{
		for(int j=0;j<n;j++)
		{
			cout<<vect[i][j]<<"   ";
		}
		cout<<endl;
	}
	for(int i=0;i<m;i++)
	{
	
	sort(vect[i].begin(),vect[i].end());
}
	for(int i=0;i<m;i++)
	{
		for(int j=0;j<n;j++)
		{
			cout<<vect[i][j]<<"  ";
		}
		cout<<endl;
	}
	
}
