#include<iostream>
using namespace std;
int  arrange(int a[],unsigned int start,unsigned int end)
{
//	int middle=(start+end)/2;
	while(start<end)
	{
		cout<<a[end];
		end++;
		cout<<a[start];
		start--;
		
	}
}

int main()
{
	int a[]={1,2,3,4,5,6,7};//4536271
	int n=sizeof(a)/sizeof(a[0]);
	cout<<a[n/2];
	arrange(a,n/2-1,n/2+1);
	
	
	
	

}
