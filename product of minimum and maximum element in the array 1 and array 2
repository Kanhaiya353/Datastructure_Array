#include<iostream>
using namespace std;
int main()
{
	int a[]={9,6,3,5,3};
	int b[]={2,7,8,1,5,};
	int q=sizeof(a)/sizeof(a[0]);
	int w=sizeof(b)/sizeof(b[0]);
	int o=INT_MIN,m=INT_MAX;
	for(int i=0;i<q;i++)
	{
		if(o<a[i])
		{
			o=a[i];
		}
	}
//	cout<<o<<" ";
	for(int j=0;j<w;j++)
	{
		if(m>b[j])
		{
			m=b[j];
		}
	}
	//cout<<m;
	cout<<o*m;
	
	
}
